{
    "version": 2,
    "builds": [
        {
            "src": "main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/provider",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "content-type"
            },
            "dest": "main.js"
        },
        {
            "src": "/api/provider/(.*)",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "content-type"
            },
            "dest": "main.js"
        },
        {
            "src": "/(.*)",
            "dest": "main.js"
        }
    ]
}
